{
  "name": "ng2-materialize",
  "version": "1.1.1",
  "description": "An Angular 2 wrap around Materialize library",
  "license": "Apache-2.0",
  "repository": "sherweb/ng2-materialize",
  "keywords": [
    "angular",
    "angular-2",
    "angular2",
    "ng",
    "ng2",
    "material",
    "material-design",
    "materialize",
    "materializecss",
    "component",
    "ui"
  ],
  "angular-cli": {},
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf -- dist inline-template node_modules",
    "e2e": "protractor",
    "lint": "tslint \"src/**/*.ts\"",
    "ng": "ng",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "start": "cd demo-app && npm start",
    "test": "ng test",
    "prebuild": "rimraf -- dist inline-template src/node_modules",
    "build": "gulp && ngc -p src/tsconfig.lib.json",
    "postbuild": "rimraf -- src/inline-template src/node_modules"
  },
  "dependencies": {
    "@types/materialize-css": "^0.98.0",
    "materialize-css": "^0.98.0"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.0-rc.4",
    "@angular/common": "^2.4.0",
    "@angular/compiler": "^2.4.0",
    "@angular/compiler-cli": "^2.4.0",
    "@angular/core": "^2.4.0",
    "@angular/forms": "^2.4.0",
    "@angular/http": "^2.4.0",
    "@angular/platform-browser": "^2.4.0",
    "@angular/platform-browser-dynamic": "^2.4.0",
    "@angular/router": "^3.4.0",
    "@angular/tsc-wrapped": "0.5.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "codelyzer": "~2.0.0",
    "core-js": "^2.4.1",
    "gulp": "^3.9.1",
    "gulp-inline-ng2-template": "^4.0.0",
    "jasmine-core": "~2.5.2",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.2.0",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-junit-reporter": "^1.2.0",
    "protractor": "~5.1.0",
    "reflect-metadata": "0.1.8",
    "rimraf": "^2.5.4",
    "rxjs": "^5.1.0",
    "ts-helpers": "^1.1.1",
    "ts-node": "~2.0.0",
    "tslint": "~4.4.2",
    "tslint-eslint-rules": "^3.2.3",
    "tslint-microsoft-contrib": "^4.0.0",
    "typescript": "~2.0.0",
    "zone.js": "^0.7.6"
  }
}
