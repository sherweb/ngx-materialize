{
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "lib": [
      "es2016",
      "dom"
    ],
    "outDir": "../out-tsc/app",
    "module": "es2015",
    "baseUrl": "",
    "types": [
      "jquery",
      "materialize-css",
      "mcustomscrollbar",
      "node",
      "prismjs"
    ],
    "paths": {
      "@angular/*": ["../node_modules/@angular/*"],
      "rxjs/*": ["../node_modules/rxjs/*"]
    }
  },
  "angularCompilerOptions": {
    "strictMetadataEmit": true
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts"
  ]
}
