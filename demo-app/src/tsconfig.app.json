{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "module": "es2015",
    "baseUrl": "",
    "types": [
      "jquery",
      "materialize-css",
      "mcustomscrollbar",
      "node",
      "prismjs"
    ],
    "paths": {
      "@angular/*": ["../node_modules/@angular/*"],
      "rxjs/*": ["../node_modules/rxjs/*"]
    }
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts",
    "**/markdown/*"
  ]
}
